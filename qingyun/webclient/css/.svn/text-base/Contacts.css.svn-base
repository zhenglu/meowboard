#Main #Left #Contact label {
	display			: block;
	font-weight		: bold;
	font-size		: 12px;
	color			: #216778;
	margin			: 4px 0px 2px 0px;
	cursor			: pointer;
}

#Main #Left #Contact tr {
	display			: block;
	text-indent		: 20px;
	width			: 160px;
	margin			: 1px 0px;
	font-size		: 11px;
	color			: #000;
	/*
	text-indent		: 5mm;
	width			: 42mm;
	*/
}

#Main #Left #Contact td:hover {
	background-color	: #DAF3F5;
	cursor			: pointer;
}

#Main #Left #Contact td {
	height			: 17px;
	width			: 115px;
	text-align		: left;
	float		: left;
	/*
	width			: 76%;
	*/
}

#Main #Left #Contact .rating {
	width			: 8px;
	text-align		: right;
	/*
	width			: 2mm;
	*/
}

#Main #Left #Contact hr {
	height			: 1px;
	margin			: 2px 15px 0px 0px;
	background-color	: #FFA200;
	color	: #FFA200;
	border			: 0px;
	/*
	margin			: 0.5mm 4mm 0mm 0mm;
	*/
}

#Main #Left #Contact #ListDiv {
	overflow-y		: auto;
	overflow-x		: hidden;
	height			: 120px;
	/*
	height			: 32mm;
	*/
}

#Main #Left #Contact #ContactInside div .UserList .UserTable{
	margin-top		: 0mm;
}

