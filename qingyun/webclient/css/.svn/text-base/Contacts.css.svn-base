/* Search user */
#Main #Left #Contact #contact_search_user {
	border-bottom		: 1px solid #216778;
	text-decoration		: none;
	font-style		: italic;
	font-size		: 10px;
	color			: #216778;
	cursor			: pointer;
}

#Main #Left #Contact #contact_search_user:hover {
	color			: #FFA200;
	border-bottom		: 1px solid #FFA200;
}

#Main #Left #Contact .contact_search_user_p {
	text-align		: center;
	padding-top		: 5px;
	padding-right		: 14px;
	padding-bottom		: 5px;
	/*
	padding-top		: 3%;
	padding-right		: 7%;
	padding-bottom		: 3%;
	*/
}

/**
* USER MENU
*/
#UserMenuDiv {
	position		: absolute;
	width			: 120px;
	padding			: 4px;
	background-color	: #FFF;
	border			: 1px solid #216778;

	z-Index			: 3;
	/*
	width			: 35mm;
	padding			: 1mm;
	*/
}

#UserMenuDiv p {
	font-style		: italic;
	font-size		: 10px;
	color			: #216778;
	cursor			: pointer;
	margin-bottom		: 1px;
}

#UserMenuDiv p:hover {
	background-color	: #DAF3F5;
}

#UserMenuDiv .option_disabled {
	color			: #C0C0C0;
}

/**
*	Invite window
*/

#InviteDiv {
	padding-top		: 30px;
}

#InviteDiv #TextDiv span {
	font-size 		: 11px;
	font-weight		: normal;
	color			: #216778;
}

#InviteDiv #TextDiv .username {
	font-size 		: 13px;
	font-weight		: bold;
}

#InviteDiv #ButtonsDiv {
	padding-top 		: 30px;
	padding-bottom 		: 5px;
	text-align		: center;
	position		: relative;
}

#InviteDiv #TextDiv {
	text-align		: center;
	position		: relative;
}

#SearchUserDiv {
	padding-top		: 15px;
}

#SearchUserDiv span {
	padding-top		: 20px;
	margin-right		: 10px;
	font-size 		: 13px;
	font-weight		: bold;
	color			: #216778;
}

#SearchUserDiv tr {
	cursor			: pointer;
	font-size 		: 12px;
	color			: #216778;
}

#SearchUserDiv td {
	width			: 160px;
}

#SearchUserDiv td:hover {
	background-color	: #DAF3F5;
	border			: 0px solid #216778;
}

#SearchUserDiv .header {
	color			: #216778;
}

#SearchUserDiv .no_found {
	padding-left		: 5px;
	font-size 		: 10px;
}

#SearchUserDiv .radio {
	border : 0px none;
}

#SearchUserDiv #TopDiv,
#SearchUserDiv #FormDiv {
	padding-left		: 20px;
}

#SearchUserDiv #OptionDiv {
	padding-top	: 22px;
	padding-left :  20px;
}

#SearchUserDiv .label {
	margin-top		: 5px;
	font-size		: 11px;
	font-weight		: normal;
}

#SearchUserDiv #ListDiv {
	text-align 		: left;
	margin-left		: 20px;
	width			: 90%;
	display			: block;
	overflow		: auto;
	height			: 58px;
}

#SearchUserDiv input{
	margin-top		: 5px;
}

#SearchUserDiv #ButtonsDiv .button {
	margin-left		: 10px;
}

#SearchUserDiv #ButtonsDiv {
	padding-top 		: 30px;
	padding-right		: 10px;
	text-align		: center;
	position		: relative;
}

#contact_loading, #contact_online_loading
{
	width			: 125px;
	height			: 35px;

	padding			: 10px;

	position		: absolute;
	background-color	: #FFF;
	border			: 1px solid #216778;

	color			: #216778;
	font-size		: 12px;

	top			: 60px;
	left			: 20px;
}
