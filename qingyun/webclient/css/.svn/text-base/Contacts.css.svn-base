#Main #Left #Contact {
	/*
	margin-bottom		: 5%;
	height			: 52mm;
	*/
	margin-bottom		: 5px;
	border			: 2px solid #216778;
	border-top : 0px;
	width			: 200px;
	height : 100%;
	margin-top : 4px;
}

#Main #Left #Contact #ContactTitle {
	font-weight		: bold;
	font-size		: 11px;
	color			: #FFF;
	/*padding		: 1mm 0mm 0mm 0mm;*/
	display			: block;
	list-style		: none;
	position		: relative;
	/*width			: 102%;*/
	width			: 204px;
	left : -2px;
	overflow : hidden;
}

#Main #Left #Contact #ContactTitle:after {
	content:".";
	display:block;
	clear:both;
	visibility:hidden;
	height:0;
	overflow:hidden;
}

#Main #Left #Contact #ContactTitle li {
	background-color	: #216778;
	cursor			: pointer;
	float			: left;
	height			: 14px;
	/*
	width			: 42%;
	padding			: 2px 2px 2px 3mm;
	*/
	width			: 98px;
	padding			: 3px 0px 2px 4px;
	font-weight : bold;
	overflow : hidden;
}

#Main #Left #Contact #ContactTitle span.bold {
	font-weight : bold;
}

#Main #Left #Contact #ContactTitle li.contact_selec {
	background-color	: #FFA200;
}

#Main #Left #Contact #UserLists {
	width : 192px;
}

#Main #Left #Contact #ContactInside ,
#Main #Left #Contact #ContactOnline {
	/*
	padding-top		: 1mm;
	padding-left		: 2mm;
	*/
	padding-top		: 5px;
	padding-left		: 8px;
	background-color	: #FFF;
	position		: relative;
	width : 100%;
	height : 100%;
	overflow : hidden;
}

/* User list */
/*
#Main #Left #Contact #Contacts {
	display			: block;
	height			: 33mm;
	padding-bottom		: 1mm;
	margin-top		: 2mm;
	overflow-y		: auto;
	overflow-x		: hidden;
	width			: 98%;
}

#Main #Left #Contact #Contacts table {
	border-spacing		: 0px;
	width			: 44mm;
}
