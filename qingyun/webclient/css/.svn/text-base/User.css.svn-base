div .UserList #order_nick
{
	color			: #216778;
	cursor			: pointer;
	font-size		: 10px;
}

div .UserList #order_nick.selected
{
	color			: #FFA200;
	font-weight             : bold !important;
	border-bottom		: 1px solid #FFA200;
}

div .UserList #order_rating
{
	color			: #216778;
	cursor			: pointer;
	position                : absolute;
	width                   : 100px;
	right                   : 3px;
	text-align              : left;
	font-size		: 10px;
	border                  : 1px solid #216778;
	/*
	width                   : 56%;
	*/
}

div .UserList #order_rating.selected
{
	border                  : 1px solid #FFA200;
	color			: #FFA200;
}

div .UserList #order_rating option.selected
{
	color			: #FFA200;
}

div .UserList #order_rating option.not_selected 
{
	color                   : #216778;
}

/*Contact list user list css*/
#ContactInside .UserList{
	padding-left		: 4px;
	/*
	padding-left		: 1mm;
	*/
}

#ListDiv div .UserList table{
	height			: 0px;
	margin-top		: 0px;
}

#ListDiv .UserList {
	padding-left		: 1px;
	padding-top		: 0px;
	/*
	padding-left		: 1mm;
	*/
}


/*Contact online user list css*/
#ContactOnline .UserList{
	padding-left		: 4px;
	height			: 141px;
	/*
	padding-left		: 1mm;
	height			: 37.7mm;
	*/
}

#ContactOnline .UserTable{
	height			: 122px;
	/*
	height			: 32mm;
	*/
}

/*Room list user list css*/
#Rooms div .UserList{
	position		: absolute;
	left			: 202px;
	width			: 150px;

	border			: 2px solid #216778;
	background-color	: #FFF;

	padding			: 4px 0px 4px 5px;

	z-index			: 1;
}

#Rooms div .UserList table{
	height			: 70px;
}

#Rooms div .UserList .UserTable{
	height			: 80px;
	/*
	height			: 22mm;
	*/
}

#Rooms div .UserList td{
	width			: 95px;
}

#Rooms div .UserList td.rating{
	width			: 8px;
	text-indent		: 10px;
}

/**
* USER MENU
*/
#UserFullNameDiv {
	position		: absolute;
	padding			: 4px;
	background-color	: #FFF;
	border			: 1px solid #216778;
	z-index	:	2;
}

#UserFullNameDiv p {
	font-style		: italic;
	font-size		: 10px;
	color			: #216778;
	cursor			: pointer;
	margin-bottom		: 1px;
}
