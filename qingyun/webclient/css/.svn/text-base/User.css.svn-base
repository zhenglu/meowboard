/*CSS default*/

div .UserList{
	padding-left		: 12px;
	padding-top		: 4px;
	font-size		: 11px;
	/*
	padding-left		: 3mm;
	padding-top		: 1mm;
	*/
}

div .UserList .UserTable{
	overflow-y		: auto;
	overflow-x		: hidden;
	margin-top		: 8px;
	/*
	margin-top		: 2mm;
	*/
}

div .UserList table{
	display			: block;
	border-spacing		: 0px;
	height			: 100px;
	width			: 155px;
	margin-top		: 8px;
	/*
	height			: 25mm;
	width			: 48mm;
	margin-top		: 2mm;
	*/
}

div .UserList tbody{
	display			: block;
	width			: 150px;
	/*
	width			: 44mm;
	*/
}


div .UserList tr{
	text-indent		: 20px;
	margin			: 1px 0px;
	height			: 16px;
	list-style		: none;
	font-size		: 11px;
	cursor			: pointer;
	/*
	text-indent		: 5mm;
	*/
}

div .UserList tr:hover {
	background-color	: #DAF3F5;
}

div .UserList td{
	width			: 155px;
	text-align		: left;
	height 			: 17px;
	/*
	width			: 41mm;
	*/
}

div .UserList td.rating{
	width			: 8px;
	text-align		: right;
	/*
	width			: 2mm;
	*/
}

